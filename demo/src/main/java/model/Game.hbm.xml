<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Game" table="Games">
        <id name="gameId" type="java.lang.Long" column="game_id">
            <generator class="increment" />
        </id>
        <property name="roomNumber" type="java.lang.Integer" column="room_number" length="20" />
    </class>
</hibernate-mapping>
