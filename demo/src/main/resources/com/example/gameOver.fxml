               <children>
                  <Label fx:id="nameLabel" text="Good game">
                     <HBox.margin>
                        <Insets left="50.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Arial Black" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Label fx:id="scoreLabel" text="Your score is:">
